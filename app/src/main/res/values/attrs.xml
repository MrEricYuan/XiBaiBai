<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScaleView">
        <attr name="modelby" format="integer">
            <enum name="by_width" value="1"></enum>
            <enum name="by_height" value="2"></enum>
        </attr>
        <attr name="multiple" format="float" />
    </declare-styleable>
    <declare-styleable name="PickListLayout">
        <attr name="isShowHeader" format="boolean" />
        <attr name="headerTag" format="string" />
        <attr name="headerContent" format="string" />
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>
    <attr name="src" format="reference"></attr>
    <declare-styleable name="CustomImageView">
        <attr name="borderRadius" />
        <attr name="type" />
        <attr name="src" />
    </declare-styleable>
</resources>